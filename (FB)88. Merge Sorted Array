/*
基本思路就是从后面空白的地方开始，因为m+n -1就是最后空白的INDEX

复杂度分析
时间复杂度 : O(n+m)。
空间复杂度 : O(1)
 */

class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int index =  m+ n -1;
        int a = m -1;
        int b = n- 1;
        while(a >=0 && b>=0){
            if(nums1[a] <=nums2[b]){
                nums1[index--] = nums2[b--];
            }else{
                nums1[index--] = nums1[a--];
            }
        }
        //着一步不需要
        /*
        while(a>=0){
            nums1[index--] = nums1[a--];
        }
        */
        while(b>=0){
            nums1[index--] = nums2[b--];
        }
        return;

    }
}
